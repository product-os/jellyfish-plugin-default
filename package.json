  },
  "engines": {
    "node": ">=14.2.0"
  },
  "description": "Default Jellyfish Plugin",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{js}"
    ],
    "modules": [
      "lib"
    ]
  },
  "ava": {
    "timeout": "10m",
    "verbose": true,
    "serial": true,
    "failWithoutAssertions": false
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "npm run doc",
      "git add README.md"
    ]
  },
  "scripts": {
    "test": "npm run doc && catch-uncommitted --skip-node-versionbot-changes && make lint && make test",
    "doc": "jsdoc2md --template doc/README.hbs lib/index.js lib/**/*.js > README.md"
  },
  "author": "Balena.io. <hello@balena.io>",
