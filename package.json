{
  "name": "@balena/jellyfish-plugin-default",
  "version": "11.0.13",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-plugin-default.git"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "description": "Default Jellyfish Plugin",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{js}"
    ],
    "modules": [
      "lib"
    ]
  },
  "ava": {
    "timeout": "10m",
    "verbose": true,
    "serial": true,
    "failWithoutAssertions": false
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "npm run doc",
      "git add README.md"
    ]
  },
  "scripts": {
    "test": "npm run doc && catch-uncommitted --skip-node-versionbot-changes && make lint && make test",
    "doc": "jsdoc2md --template doc/README.hbs lib/index.js lib/**/*.js > README.md"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.1.39",
    "@balena/jellyfish-client-sdk": "^4.4.17",
    "@balena/jellyfish-environment": "^4.1.31",
    "@balena/jellyfish-logger": "3.0.1",
    "@balena/jellyfish-plugin-base": "^2.1.131",
    "axios": "^0.21.1",
    "bluebird": "^3.7.2",
    "fast-json-patch": "^3.0.0-1",
    "front-sdk": "^0.8.1",
    "geoip-lite": "^1.4.2",
    "intercom-client": "^2.11.2",
    "is-uuid": "^1.0.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "lru-cache": "^6.0.0",
    "marked": "^1.2.9",
    "native-url": "^0.3.4",
    "node-jose": "^2.0.0",
    "pluralize": "^8.0.0",
    "randomstring": "^1.2.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@balena/ci-task-runner": "^0.2.158",
    "@balena/jellycheck": "^0.1.1",
    "@balena/jellyfish-action-library": "^11.0.1",
    "@balena/jellyfish-core": "^3.0.0",
    "@balena/jellyfish-sync": "^6.0.16",
    "@balena/jellyfish-test-harness": "^3.0.0",
    "ava": "^3.15.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "catch-uncommitted": "^2.0.0",
    "depcheck": "^1.4.1",
    "deplint": "^1.1.3",
    "eslint": "^7.27.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^35.1.0",
    "eslint-plugin-lodash": "^7.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "express": "^4.17.1",
    "form-data": "^4.0.0",
    "jsdoc-to-markdown": "^7.0.1",
    "lint-staged": "^11.0.0",
    "morgan": "^1.10.0",
    "nock": "^13.0.11",
    "proxyquire": "^2.1.3",
    "simple-git-hooks": "^2.4.1",
    "sinon": "^11.1.1",
    "typed-errors": "^1.1.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}
