{
  "name": "@balena/jellyfish-plugin-default",
  "version": "8.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-plugin-default.git"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "description": "Default Jellyfish Plugin",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{js}"
    ],
    "modules": [
      "lib"
    ]
  },
  "ava": {
    "timeout": "10m",
    "verbose": true,
    "serial": true,
    "failWithoutAssertions": true
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "npm run doc",
      "git add README.md"
    ]
  },
  "scripts": {
    "test": "npm run doc && catch-uncommitted --skip-node-versionbot-changes && make lint && make test",
    "doc": "jsdoc2md --template doc/README.hbs lib/index.js lib/**/*.js > README.md"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.0.67",
    "@balena/jellyfish-client-sdk": "^2.18.6",
    "@balena/jellyfish-environment": "^3.0.1",
    "@balena/jellyfish-logger": "2.1.22",
    "@balena/jellyfish-plugin-base": "^2.0.29",
    "@balena/jellyfish-uuid": "^1.0.74",
    "@octokit/auth-app": "^2.11.0",
    "@octokit/plugin-retry": "^3.0.7",
    "@octokit/plugin-throttling": "^3.4.1",
    "@octokit/rest": "^18.3.5",
    "bluebird": "^3.7.2",
    "fast-json-patch": "^3.0.0-1",
    "front-sdk": "^0.8.1",
    "geoip-lite": "^1.4.2",
    "intercom-client": "^2.11.0",
    "is-uuid": "^1.0.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "lru-cache": "^6.0.0",
    "marked": "^1.2.9",
    "native-url": "^0.3.4",
    "node-jose": "^2.0.0",
    "pluralize": "^8.0.0",
    "randomstring": "^1.1.5",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@balena/ci-task-runner": "^0.2.112",
    "@balena/jellycheck": "^0.1.1",
    "@balena/jellyfish-action-library": "^8.1.19",
    "@balena/jellyfish-core": "^2.12.20",
    "@balena/jellyfish-sync": "^5.2.30",
    "@balena/jellyfish-test-harness": "^1.1.58",
    "ava": "^3.15.0",
    "body-parser": "^1.19.0",
    "catch-uncommitted": "^1.6.2",
    "depcheck": "^1.4.0",
    "deplint": "^1.1.3",
    "eslint": "^7.22.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.3.0",
    "eslint-plugin-lodash": "^7.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "express": "^4.17.1",
    "jsdoc-to-markdown": "^7.0.0",
    "lint-staged": "^10.5.4",
    "morgan": "^1.10.0",
    "nock": "^13.0.11",
    "simple-git-hooks": "^2.2.0",
    "sinon": "^9.2.4",
    "typed-errors": "^1.1.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}
